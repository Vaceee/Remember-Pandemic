          "type": "Arrays",
          "content": [
            {
              "id": "int",
              "usr_name": "str",
              "content": "str",
              "time": "str",
              "like_cnt": "int",
              "reply": {
                "type": "Object",
                "content": {
                  "first_reply": {
                    "type": "Object",
                    "content": {
                      "id": "int",
                      "usr_name": "str",
                      "content": "str",
                      "time": "str",
                      "like_cnt": "int"
                    }
                  },
                  "reply_cnt": "int"
                }
              }
            }
          ],
          "排序方式": {
            "关键字": "time",
            "顺序": "升序"
          }
        },
        "merged": {
          "type": "Object",
          "required": false,
          "content": {
            "to": "str",
            "title": "str"
          }
        }
      }
    },
    "done": true
  },
