{
  "检查登录状态": {
    "path": "/login",
    "method": "GET",
    "return": {
      "type": "Object",
      "content": {
        "status": "str",
        "usr_name": {
          "type": "str",
          "required": false
        },
        "usr_gender": {
          "type": "str",
          "value": ["F", "M"],
          "required": false
        },
        "usr_no": {
          "type": "str",
          "required": false
        },
        "usr_userlevel": {
          "type": "int",
          "required": false
        }
      }
    },
    "done": true
  },
  "登录": {
    "path": "/login",
    "method": "POST",
    "parameters": {
      "usr_no": "str",
      "usr_password": "str"
    },
    "return": {
      "//": "返回与GET方法时保持一致",
      "type": "Object",
      "content": {
        "status": "str",
