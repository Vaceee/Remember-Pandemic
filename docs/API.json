          "type": "str",
          "required": false
        },
        "usr_gender": {
          "type": "str",
          "value": ["F", "M"],
          "required": false
        },
        "usr_no": {
          "type": "str",
          "required": false
        },
        "usr_userlevel": {
          "type": "int",
          "required": false
        }
      }
    },
    "done": true
  },
